{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "files": ["**"],
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "useGitignore": true,
  "ignorePaths": [
    "*.pyc",
    ".git/**",
    "dist/**",
    "node_modules/**",
    "package-lock.json",
    "pnpm-lock.yaml",
    "yarn.lock"
  ],
  "words": [
    "autocrlf",
    "changenote",
    "contentfolder",
    "dpkg",
    "esbenp",
    "isaacscript",
    "packagejson",
    "publishedfileid",
    "sarisia",
    "steamapps",
    "steamcmd",
    "STEAMCMDDIR",
    "workshopbuilds",
    "workshopitem",
    "Zamiell"
  ]
}
