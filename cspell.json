{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "files": [
    "**"
  ],
  "enableFiletypes": [
    "*"
  ],
  "enableGlobDot": true,
  "useGitignore": true,
  "ignorePaths": [
    ".git/**"
  ],
  "words": [
    "autocrlf",
    "contentfolder",
    "dpkg",
    "isaacscript",
    "publishedfileid",
    "sarisia",
    "steamapps",
    "steamcmd",
    "STEAMCMDDIR",
    "technote",
    "workshopbuilds",
    "workshopitem",
    "Zamiell",
    "zamts"
  ]
}
